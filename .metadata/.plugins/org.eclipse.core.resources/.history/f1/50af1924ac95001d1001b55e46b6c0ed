# datasource properties
spring.datasource.url=jdbc:mysql://localhost:3306/Naresh_It
spring.datasource.username=sachin
spring.datasource.password=password
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver


spring.datasource.dbcp2.max-total=100
spring.datasource.dbcp2.initial-size=10
spring.datasource.dbcp2.max-conn-lifetime-millis=100000
#specify datasource type
spring.datasource.type=org.apache.commons.dbcp2.BasicDataSource