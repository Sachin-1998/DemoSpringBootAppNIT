package com.nit.runners;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the subjects");
		int i=0;
		double avg=0;
		int sub=0;
		int max=0;
		int result=0;
		sub=sc.nextInt();
		int [] a=new int[sub];
		for(i=0;i<a.length;i++)
		{
			a[i]=sc.nextInt();
			break;
		}
		for(i=0;i<a.length;i++)
		{
			if(a[i]>max)
			{
				result=a[i];
				break;
			}
		}
		System.out.println(result);
		
	}

}
